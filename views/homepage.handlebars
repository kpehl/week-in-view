<div>
    {{userData.username}}
    {{#each userData.events}}
    <h3>{{ title }}</h3>
    <p>{{ text }}</p>
    <p>{{hour}}:{{minute}}, {{month}}/{{day}}/{{year}}</p>
    {{/each}}
</div>
<div>
    {{#each userData.tasks}}
    <p>{{ text }} {{ status }}</p>
    {{/each}}
</div>

<div id='calendar'></div>

<script src="/javascript/calendar.js"></script>